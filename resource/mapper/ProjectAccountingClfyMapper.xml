<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.ldxx.dao.ProjectAccountingClfyDao" >

	<insert id="addProjectAccountingClfy" parameterType="java.util.List">
		insert into project_accounting_clfy(pa_id,mater_name,mater_type,mater_mode,mater_unit,mater_number,mater_price) values
  			<foreach collection="accountingClfy" item="item"  index="index" separator=",">
  				(#{item.paId},#{item.materName},#{item.materType},#{item.materMode},#{item.materUnit},#{item.materNumber},#{item.materPrice})
  			</foreach>
	</insert>
	
	<select id="getProjectAccountingClfyById" resultType="ProjectAccountingClfy">
		select * from project_accounting_clfy where pa_id=#{id}
	</select>

</mapper>