<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.ldxx.dao.ClOutDao" >
  	<insert id="addClOut" parameterType="clout">
  		insert into cl_out(out_id,cl_name,cl_type,mode_type,
  			unit,num,price,sum_price,get_time,get_person,infos)
  		values(#{out.outId},#{out.clName},#{out.clType},#{out.modeType},
  				   #{out.unit},#{out.num},#{out.price},#{out.sumPrice},#{out.getTime},#{out.getPerson},#{out.infos})
  	</insert>
  	
  	<select id="selectClOut" resultType="clout">
  		select * from cl_out
  	</select>
</mapper>