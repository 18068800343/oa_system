ul.sidebar-menu{
    border-top:1px solid #3d485a;
    padding: 0;
}
ul.sidebar-menu li{
    box-sizing: border-box;
/*    height:40px;*/
    line-height:40px;
    background: #30394a;
}
ul.sidebar-menu li a{
    display:block;
    box-sizing: border-box;
    width:100%;
    height:100%;
    color:#9facbd;
    text-align:left;
}
ul.sidebar-menu li a.active{
    background:#30394a;
    border-left:6px solid #53aadf;
    color:#fff;
    padding-left:14px;
}
ul.sidebar-menu li  ul li a.active{
    background:#4A708B;
    border-left:6px solid #53aadf;
    color:#fff;
}
/* 三级菜单 */
ul.sidebar-menu li>ul>li>ul>li a{
    display:block;
    box-sizing: border-box;
    width:100%;
    height:100%;
    color:#9facbd;
    text-align:left;
    padding-left:35px;
}
ul.sidebar-menu li>ul>li>ul>li a.active{
    color:#fff;
}
/* 三级菜单结束*/
ul.sidebar-menu li ul {
    padding-left: 0px;
}
ul.sidebar-menu li li{
    line-height: 30px;
    background: #3e4758;
}
ul.sidebar-menu li ul strong{
    font-size: 16px;
    margin-right: 3px;
    font-weight: normal;
}
ul.sidebar-menu > li > a:hover{
    background:#30394a;
    padding-left:14px;
    border-left:6px solid #53aadf;
    color:#fff;
}

ul.sidebar-menu li li a:hover{
    color:#fff!important;
}

ul.sidebar-menu li .on{
   color:#fff!important; 
}
