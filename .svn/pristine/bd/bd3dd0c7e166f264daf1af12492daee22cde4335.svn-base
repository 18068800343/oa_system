
.content {width:100%;margin:38px auto;border:1px solid #dae0e6;height:615px;}
.content_map_title{width:100%;height:32px;border-bottom:1px solid #e7e7e9;background:#f2f2f2; float:right;}
.btn_map{width:50px; height:27px; line-height:27px; border:1px solid #bbbbbb; text-align:center; cursor:pointer; margin-top:2px; float:right; margin-right:35px; font-size:13px;}
.btn_select{width:10%;height:27px; line-height:27px;border:1px solid #bbbbbb; text-align:left;padding-left:8px; cursor:pointer;margin-top:2px; float:right; background:#fff;font-size:13px;margin-right:15px;}
.btn_sousuo{width:315px;height:27px;border:1px solid #bbbbbb; float:right;margin-top:2px;margin-right:15px;}
.btn_sousuo img{ margin-top:4px; position:absolute;}
.btn_sousuo input{background: #f2f2f2;
height: 24px;
width: 292px;
line-height: 20px;}
.btn_sousuo p{float:right;width:30px;height:27px;line-height:27px; cursor:pointer;margin-right:5px;}
.left_map{height:582px;width:0px; position:absolute;margin-top:33px; max-height:582px;overflow:auto; font-size:13px;}
.left_map ul{width: 300px;position:absolute;z-index:200; }

.look_map{ width:100%;height:100%; float:right;}

.xz_select{
	display: none;
	background: #fff;
	padding: 10px;
}
.xz_select li{
	list-style: none;
}
.xz_select li a{
	text-decoration: none;
	color: #626262;
}
#list-item li{
	list-style: none;
	cursor: pointer;
	padding: 10px;
}
.left_p1,.left_p2{
	display: inline-block;
	margin-right: 20px;
}
.left_map_li_1 a{
	color: #2a6496;
	cursor: pointer;
}
#list-item li:hover{
	background: #eee;
}
#list-item li:hover a{
	text-decoration: underline!important;
}

.w_close{
	display: none;
	line-height: 32px;
	margin-left: 10px;
}

