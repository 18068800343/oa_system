				if(json>0){
					$('#mainContent').DataTable().row(dom).remove().draw(false); //删除行立即显示
					toastr.success("删除成功")
				}else{
					toastr.error("删除失败")
				}
			}
		})
	})
	
	//修改项目印章刻制申请回显
	var xiugaiId;
	$("#mainContent").delegate("#Changedanwei","click",function(){
		dom=$(this).parents("tr");
		var data=$('#mainContent').DataTable().row(dom).data();
		xiugaiId=data.smId;
		$.ajax({
			type:"post",
			url:getContextPath()+"/prjSeal/selectPrjSealById",
			dataType:"json",
			async:false,
			data:{
				smId:data.smId
			},
			success:function(json){
				//console.log(json);
				$("#xgxiangmu").val(json.prjId);
				$("#xgbumen").val(json.omId);
				//$("#xgyinzhangshouquanren").val(data.uName);
				
				initSelectOnEdit(json.signetMan)
				
				$("#xgyear").val(json.applicationDate);
				$("#xgbianhao").val(json.smNo);
				$("#xgquancheng").val(json.signetName);
			}
		})
	})
	//保存修改
	function checkxiugai(){
		var xgxiangmu=$("#xgxiangmu").val();
		var xgbumen=$("#xgbumen").val();
		var xgyinzhangshouquanren=$("#xgyinzhangshouquanren").val();
		var xgyear=$("#xgyear").val();
		var xgbianhao=$("#xgbianhao").val();
		var xgquancheng=$("#xgquancheng").val();
		if(xgquancheng==''){
			toastr.error("专用章全称为空");
			return;
		}
		
		$.ajax({
			type:"post",
			url:getContextPath()+"/prjSeal/updatePrjSealById",
			dataType:"json",
			data:{
				smId:xiugaiId,
				prjId:xgxiangmu,
				omId:xgbumen,
				signetMan:xgyinzhangshouquanren,
				applicationDate:xgyear,
				smNo:xgbianhao,
				signetName:xgquancheng
			},
			success:function(json){
				if(json.result==0){
					toastr.error("修改失败")
				}else if(json.result==1){
					//location.reload([true]);
					$('#change').modal('hide');
					toastr.success("修改成功")
					initTable();
				}
			}
		})
	}
	
